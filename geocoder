import requests
import json
def geocoded_address(street_address, city, state):
    address = (f"{street_address.title()} {city.title()} {state.upper()}")
    api_key = 'AIzaSyDFYbFdaLtINSgTPkL4QWNiXcliknuUE_I'
    base_url = 'https://maps.googleapis.com/maps/api/geocode/json?'
    params = {
    'key': api_key,
    'address': address
    }
    response = requests.get(base_url, params=params)
    data = response.json() 
    lattitude = (data['results'][0]['geometry']['location']['lat'])
    longitude = (data['results'][0]['geometry']['location']['lng'])
    print(f"{longitude}, {lattitude}")
